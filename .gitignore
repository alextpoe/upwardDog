# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore node_modules
node_modules

# Ignore bundle.js.
/bundle.js

# Ignore application configuration
/config/application.yml

# Ignore DS_store and byebug files
.DS_Store
.byebug_history
